from table.apps.meal.models import *

number_of_mixxes = 5  # This is how many more you want.

for i in range(0, number_of_mixxes):
    new_mixx = Mixx.objects.get(pk=22)  # Use the id of the Mixx to duplicate.
    new_mixx.id = None
    new_mixx.occurring_at = new_mixx.occurring_at + datetime.timedelta(days=1+i)
    new_mixx.invites_expire = new_mixx.invites_expire + datetime.timedelta(days=1+i)
    new_mixx.save()
